{
  "name": "Store",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "created": {
      "type": "date",
      "required": true
    },
    "businessId": {
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "business": {
      "type": "belongsTo",
      "model": "Business",
      "foreignKey": "businessId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
